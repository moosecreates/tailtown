{
  "name": "tailtown",
  "version": "0.1.0",
  "description": "Pet Resort Management System for Tailtown",
  "private": true,
  "workspaces": [
    "services/*",
    "frontend",
    "shared/*"
  ],
  "scripts": {
    "start": "docker-compose up",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "test": "lerna run test",
    "build": "lerna run build"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "lerna": "^6.6.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/system": "^5.14.20",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
