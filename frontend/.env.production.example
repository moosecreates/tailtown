# Tailtown Frontend - Production Environment
# Copy this file to .env.production and fill in your actual values

# API URLs - Replace with your production backend URLs
REACT_APP_API_URL=https://api.yourdomain.com
REACT_APP_RESERVATION_API_URL=https://api.yourdomain.com

# Tenant Configuration
REACT_APP_TENANT_ID=your-tenant-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# API Timeout (milliseconds)
REACT_APP_API_TIMEOUT=30000

# Environment
NODE_ENV=production

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Optional: Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_MIXPANEL_TOKEN=

# Optional: Error Tracking
REACT_APP_SENTRY_DSN=

# Optional: Feature Flags Service
REACT_APP_LAUNCH_DARKLY_KEY=
