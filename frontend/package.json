{
  "name": "tailtown-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/system": "5.14.20",
    "@mui/x-date-pickers": "^8.17.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.28",
    "@types/react": "18.2.64",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "18.2.21",
    "@types/yup": "^0.32.0",
    "ajv": "^8.12.0",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "formik": "^2.4.8",
    "http-proxy-middleware": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-datepicker": "^8.8.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "6.22.3",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "typescript": "^5.4.2",
    "web-vitals": "^5.1.0",
    "yup": "^1.6.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "FAST_REFRESH=false GENERATE_SOURCEMAP=false craco build",
    "build:analyze": "GENERATE_SOURCEMAP=false craco build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:coverage:watch": "craco test --coverage",
    "eject": "react-scripts eject",
    "verify-build": "node scripts/check-build.js",
    "predeploy": "npm run verify-build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/dom": "^10.4.1",
    "@types/lodash": "^4.17.16",
    "@types/recharts": "^2.0.1",
    "compression-webpack-plugin": "^11.1.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
